generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  name  String?
  avatarUrl  String?
  email String  @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  @@map(name: "users") 
}